<p id="notice"><%= notice %></p>

<h1>Bicyclecategories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bicyclecategories.each do |bicyclecategory| %>
      <tr>
        <td><%= bicyclecategory.name %></td>
        <td><%= link_to 'Show', bicyclecategory %></td>
        <td><%= link_to 'Edit', edit_bicyclecategory_path(bicyclecategory) %></td>
        <td><%= link_to 'Destroy', bicyclecategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bicyclecategory', new_bicyclecategory_path %>
